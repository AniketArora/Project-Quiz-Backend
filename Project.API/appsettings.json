{
  "Tokens": {
    "Key": "DekeyvoorJWTtokenconfiguratie", //symmetrische JWT key
    "Issuer": "https://localhost:44344 ", //onze https website
    "Audience": " https://localhost ", //website die token accepteert
    //(vaak dezelfde als de issuer)
    "Expires": 60 //minuten
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=Project;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*"
  }
